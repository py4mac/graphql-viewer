    "version": "0.2.0",
    "configurations": [
      {
        "name": "backend",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/examples/backend.py",
        "console": "integratedTerminal",
        "env": {
          "PYTHONPATH": "${workspaceFolder}"
        }
      },{
      "name": "frontend",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/examples/frontend.py",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "args": ["init-db"]
    },{
      "name": "run",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/graphv/cli.py",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "args": ["run"]
    }
    ]
  }
  